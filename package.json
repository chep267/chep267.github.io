        "react-hook-form": "^7.48.2",
        "react-intl": "^6.5.5",
        "react-router-dom": "^6.20.1",
        "react-tsparticles": "^2.12.2",
        "tsparticles-engine": "^2.12.0",
        "tsparticles-slim": "^2.12.0",
        "yup": "^1.3.2"
    },
    "devDependencies": {
        "@tanstack/eslint-plugin-query": "^5.12.1",
