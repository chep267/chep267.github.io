        "@mui/styles": "^5.14.20",
        "@mui/x-date-pickers": "^6.18.3",
        "@tanstack/react-query": "^5.12.2",
        "axios": "^1.6.2",
        "classnames": "^2.3.2",
        "dayjs": "^1.11.10",
        "firebase": "^10.7.1",
        "gh-pages": "^6.1.0",
        "js-cookie": "^3.0.5",
        "mui-tel-input": "^5.0.0",
        "react": "^18.2.0",
